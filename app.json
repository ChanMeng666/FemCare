{
  "expo": {
    "name": "FemCare",
    "slug": "FemCare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/app-icon-192.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/app-icon-192.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/app-icon-192.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/app-icon-48.png"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
